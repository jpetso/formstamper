{
  "name": "formstamper",
  "productName": "Formstamper",
  "version": "0.0.1",
  "description": "GUI application (using Electron) for batch filling PDF forms",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "keywords": [],
  "author": "jpetso",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "formstamper"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "jpetso",
        "name": "formstamper"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "formstamper"
      }
    }
  },
  "dependencies": {
    "@hot-loader/react-dom": "npm:@hot-loader/react-dom@^16.8.3",
    "canvas": "^2.3.1",
    "command-exists": "^1.2.8",
    "csv-parse": "^4.3.3",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "node-pdftk": "^2.0.2",
    "pdfjs-dist": "^2.0.943",
    "react": "^16.8.3",
    "react-dom": "npm:@hot-loader/react-dom@^16.8.3",
    "react-hot-loader": "^4.7.2",
    "systeminformation": "^4.5.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7"
  }
}
